{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "ts-node-dev --transpile-only 'src/index.ts' | pino-pretty -C",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "jest --passWithNoTests",
    "test:functional": "jest --projects ./test --runInBand --passWithNoTests",
    "test:staged": "npm run test",
    "test:push": "jest --passWithNoTests --coverage && jest --projects ./test --runInBand --passWithNoTests --coverage",
    "husky:prepare": "husky install"
  },
  "engines": {
    "node": "18"
  },
  "_moduleAliases": {
    "@src": "src/",
    "@test": "test/"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Julio Zittei",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nock": "^13.3.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@prisma/client": "^4.15.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.4",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "pino": "^8.14.1",
    "swagger-ui-express": "^4.6.3",
    "zod": "^3.21.4"
  }
}
